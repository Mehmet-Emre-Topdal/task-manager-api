{
    "name": "mangolab",
    "version": "1.0.0",
    "description": "Task Management API with Express.js",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "vitest run --coverage",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "docker:build": "docker build -t mangolab-api .",
        "docker:run": "docker run -p 3000:3000 mangolab-api",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:dev": "docker-compose up app-dev",
        "docker:logs": "docker-compose logs -f"
    },
    "keywords": [
        "express",
        "nodejs",
        "mangolab",
        "task-management",
        "api"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "dotenv": "^16.3.1",
        "cors": "^2.8.5",
        "yup": "^1.3.3",
        "swagger-ui-express": "^5.0.0",
        "swagger-jsdoc": "^6.2.8",
        "morgan": "^1.10.0",
        "helmet": "^7.1.0",
        "express-rate-limit": "^7.1.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.1",
        "vitest": "^1.2.0",
        "@vitest/coverage-v8": "^1.2.0",
        "supertest": "^6.3.3"
    }
}